{
  "name": "triton",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:https": "vite --host --https",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "ANALYZE=true vite build",
    "build:inspect": "vite build --mode development && npx vite-plugin-inspect",
    "preview": "vite preview",
    "preview:https": "vite preview --https",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "type-check:watch": "vue-tsc --build --watch",
    "lint": "eslint . --fix",
    "lint:check": "eslint . --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:full": "rm -rf dist node_modules/.vite node_modules package-lock.json",
    "reinstall": "npm run clean:full && npm install",
    "test:build": "npm run build && npm run preview",
    "check-all": "run-p lint:check type-check",
    "check-all:format": "run-p lint:check format:check type-check",
    "prepare": "npm run type-check"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.1",
    "@primevue/icons": "^4.3.6",
    "@tailwindcss/vite": "^4.1.11",
    "@vueuse/core": "^13.5.0",
    "@vueuse/head": "^2.0.0",
    "pinia": "^3.0.3",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.6",
    "tailwindcss": "^4.1.11",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@primevue/auto-import-resolver": "^4.3.6",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "^10.3.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "simple-git-hooks": "^2.12.0",
    "terser": "^5.43.1",
    "typescript": "~5.8.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^3.0.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass,less,styl,html,json,jsonc,yml,yaml,md}": [
      "prettier --write"
    ]
  }
}
